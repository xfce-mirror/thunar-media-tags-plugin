msgid "Artist - Title"
msgstr "アーティスト - タイトル"

#: ../thunar-plugin/tag-renamer.c:61
msgid "Track - Title"
msgstr "曲 - タイトル"

#: ../thunar-plugin/tag-renamer.c:62
msgid "Track - Artist - Title"
msgstr "曲 - アーティスト - タイトル"

#: ../thunar-plugin/tag-renamer.c:63
msgid "Track. Title"
msgstr "曲. タイトル"

#: ../thunar-plugin/tag-renamer.c:64
msgid "Track. Artist - Title"
