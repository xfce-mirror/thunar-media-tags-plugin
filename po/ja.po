msgid "<b>Filesize:</b>"
msgstr "<b>ファイルサイズ:</b>"

#: ../thunar-plugin/audio-tags-page.c:1051
msgid "<b>MIME Type:</b>"
msgstr "<b>MIMEタイプ:</b>"

#: ../thunar-plugin/audio-tags-page.c:1064
msgid "<b>Bitrate:</b>"
